{
  "$schema": "https://ui.shadcn.com/schema.json",
  "style": "default",
  "rsc": true,
  "tsx": false,
  "tailwind": {
    "config": "tailwind.config.js",
    "css": "src/index.css",
    "baseColor": "slate",
    "cssVariables": true,
    "prefix": ""
  },
  "aliases": {
    "components": "@/components",
    "utils": "@/lib/utils"
  },
  "lint": {
    "enabled": true,
    "fix": true
  },
  "format": {
    "enabled": true,
    "options": {
      "semi": true,
      "tabWidth": 2,
      "trailingComma": "all"
    }
  }
